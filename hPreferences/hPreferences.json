{
    plugin : {
        name : "Preferences",
        library : {
            name : "Preferences Library"
        },
        service : {
            name : "Preferences Service",
            methods : [
                "save",
                "get"
            ]
        }
    },
    file : {
        title : "Hot Toddy Preferences",
        path : "/Applications/Preferences/index.html",
        directory : {
            properties: {
                isApplication: 1,
                icon : {
                    name : "Preferences.png",
                    icns : "PrefApp.icns",
                    mime : "plugins/preferences"
                }
            }
        },
        user : {
            id : 1,
            permissions : {
                owner : "rw"
            }
        }
    }
}